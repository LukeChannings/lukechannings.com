{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", ".cache/types"]
}
